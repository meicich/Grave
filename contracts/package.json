{
  "name": "grave-contract",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/meicich/Grave.git",
  "author": "",
  "license": "private",
  "private": true,
  "scripts": {
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test",
    "test:gas": "REPORT_GAS=true yarn hardhat test",
    "csize": "yarn hardhat size-contracts",
    "format:sol": "yarn solhint --fix --max-warnings 5 \"contracts/**/*.sol\"",
    "format:js": "eslint --fix './{test,scripts}/**/*.ts' && prettier --write './{test,scripts,contracts}/**/*.{ts,sol}'",
    "format": "yarn format:sol && yarn format:js",
    "deploy": "yarn hardhat run --network goerli scripts/deploy.ts",
    "abi": "yarn hardhat export-abi",
    "start": "node ddd.mjs"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.7.1",
    "hardhat": "^2.13.0",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.0",
